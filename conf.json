{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["source"],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "private": true,
        "destination": "./jsdoc/"
    }
}